    (method protected final (thumb-rect))
    (method protected override (mouse-activate?))
    (method protected override (mouse-down evt))
    (method protected override (drag-move evt))
    (method protected override (drag-up evt . rest))
    (method protected final (get-flat-color))
    (method protected final (get-background-color))
    (method protected override (draw surface <Surface> context))
    (method protected override (get-look))
    (method protected override (get-opaque?))
    (method protected final (setup-autorepeat action))
    (method protected final (kill-autorepeat)))
  (class Scrollbar-Descriptor extends View-Descriptor
    (method meta protected override (set-class-form form))
    (method meta protected override (get-class-form))
    (method meta protected override (get-class-forms))))

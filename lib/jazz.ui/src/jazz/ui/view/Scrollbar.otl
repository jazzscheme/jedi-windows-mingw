(module jazz.ui.view.Scrollbar jazz
  (import (jazz.geometry))
  (import (jazz.graphic))
  (import (jazz.jml))
  (import (jazz.library))
  (import (jazz.ui))
  (import (jazz.ui.skin))
  (import (jazz.ui.view))
  (import (jazz.designer))
  (class Scrollbar extends View
    (method meta protected override (get-class-descriptor (context: context (unspecified))))
    (property orientation accessors generate)
    (property pos <fx> accessors generate)
    (property range <Range> accessors generate)
    (property line-height <fx> accessors generate)
    (property page-height <fx> accessors generate)
    (property flat? <bool> accessors generate)
    (property scroll-proc accessors generate)
    (slot tracking?)
    (slot tracking-start)
    (slot tracking-start-pos)
    (slot autorepeat-pulse)
    (method protected override (print output readably))
    (method public final (set-info min max line page))
    (method public final (get-client))
    (method protected final (call-scroll action (thumb-pos (unspecified))))
